{
    "description": "Build an Extension!",
    "manifest_version": 2,
    "name": "Vue Chrome Extension",
    "version": "1.0.0",
    "icons": {
        "48": "assets/img/logo.png"
    },
    "browser_action": {
        "default_icon": "assets/img/logo.png",
        "default_title": "Opens MyExtension",
        "default_popup": "app_firefox.html"
    },
    "permissions": ["tabs"]
}
